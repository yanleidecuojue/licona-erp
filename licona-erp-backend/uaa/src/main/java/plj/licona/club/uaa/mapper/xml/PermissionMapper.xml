<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="plj.licona.club.uaa.mapper.PermissionMapper">
    <select id="findAdminMenus" resultType="plj.licona.club.uaa.entity.Permission">
        select id, interface_uri
        from permission p
        where p.interface_uri is not null;
    </select>
</mapper>
